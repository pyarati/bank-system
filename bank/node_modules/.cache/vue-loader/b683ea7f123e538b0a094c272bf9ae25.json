n((result) => {\n          console.log(\"result:\", result);\n          let data = result.data.data;\n          localStorage.setItem(\"id\", data.id)\n          localStorage.setItem(\"access_token\",'Bearer '+data.access_token);\n          this.$router.push({name:\"home\"});\n      })\n      .catch((e) => {\n        console.log('error:', e);\n      });\n    }\n  },\n  mounted(){\n    let user = localStorage.getItem(\"id\");\n    if (user) {\n      this.$router.push({name:'home'})\n    }\n  }\n};\n</script>\n\n<style>\n</style>"]}]}