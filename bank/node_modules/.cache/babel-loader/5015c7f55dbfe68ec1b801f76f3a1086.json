BAyCA;AACA,QAAA,IAAA,GAAA,YAAA,CAAA,OAAA,CAAA,UAAA,CAAA;;AACA,QAAA,IAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA;AACA;AA9CA,CAAA","sourcesContent":["<template>\n  <div class=\"registration\">\n    <img src=\"../assets/opcito.png\">\n    <h1>Sign Up</h1>\n    <input type=\"text\" v-model=\"first_name\" placeholder=\"Enter first name\" />\n    <input type=\"text\" v-model=\"last_name\" placeholder=\"Enter last name\" />\n    <input type=\"text\" v-model=\"address\" placeholder=\"Enter address\" />\n    <input\n      type=\"text\"\n      v-model=\"mobile_number\"\n      placeholder=\"Enter mobile number\"\n    />\n    <input type=\"email\" v-model=\"email_id\" placeholder=\"Enter email address\" />\n    <input type=\"password\" v-model=\"password\" placeholder=\"Enter password\" />\n\n    <div style=\"margin-top: 12px; margin-bottom: 12px\">\n      <label>Choose user type:  </label>\n      <label><span> Admin</span>\n        <input\n          style=\"height: 14px; width: 30px; display: inline\"\n          type=\"radio\"\n          v-model=\"selectedValue\"\n          name=\"usertype\"\n          value=\"1\"\n        />\n      </label>\n      <label>Customer</label>\n      <input\n        style=\"height: 14px; width: 30px; display: inline\"\n        type=\"radio\"\n        v-model=\"selectedValue\"\n        name=\"usertype\"\n        value=\"2\"\n      />\n    </div>\n\n    <button type=\"button\" v-on:click=\"signup()\">Sign Up</button>\n\n    <p>\n      <router-link to=\"/\">Login</router-link>\n    </p>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"signup\",\n  data: function () {\n    return {\n      first_name: \"\",\n      last_name: \"\",\n      address: \"\",\n      mobile_number: \"\",\n      email_id: \"\",\n      password: \"\",\n      selectedValue: \"\",\n    };\n  },\n  methods: {\n    signup() {\n      const options = {\n        headers: { \"Access-Control-Allow-Origin\": \"*\" },\n      };\n      axios.post(\n          \"http://0.0.0.0:5000/user\",\n          {\n            first_name: this.first_name,\n            last_name: this.last_name,\n            address: this.address,\n            mobile_number: this.mobile_number,\n            email_id: this.email_id,\n            password: this.password,\n            user_type_id: Number(this.selectedValue),\n          },\n          options\n        )\n        .then((result) => {\n          console.log(\"result:\", result);\n          //let data = result.data.data;\n          this.$router.push({ name: \"home\" });\n        })\n        .catch((e) => {\n          console.log('error:', e);\n        });\n    },\n  },\n  mounted() { \n    let user = localStorage.getItem(\"email_id\");\n    if (user) {\n        this.$router.push({name:'home'})\n    }\n  },\n};\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src/components"}]}