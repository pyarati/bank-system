rcesContent":["<template>\n  <div class=\"login\">\n    <img src=\"../assets/opcito.png\">\n    <h1>Log In</h1>\n    <input type=\"email\" v-model=\"email_id\" placeholder=\"Enter email address\" />\n    <input type=\"password\" v-model=\"password\" placeholder=\"Enter password\" />\n\n    <button type=\"button\" v-on:click=\"login()\">Login</button>\n\n    <p>\n      <router-link to='/signup'>Signup</router-link>\n    </p>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: \"login\",\n  data() {\n    return {\n      email_id: \"\",\n      password: \"\",\n    };\n  },\n  methods:{\n    login(){\n      const options = {\n        headers: { \"Access-Control-Allow-Origin\": \"*\" },\n      };\n      axios.post('http://0.0.0.0:5000/login', {\n        email_id: this.email_id,\n        password: this.password,\n      },\n      options\n      )\n      .then((result) => {\n          console.log(\"result:\", result);\n          let data = result.data.data;\n          localStorage.setItem(\"email_id\",this.email_id)\n          localStorage.setItem(\"access_token\",JSON.stringify(data.access_token));\n          this.$router.push({ name: \"home\" });\n      })\n      .catch((e) => {\n        console.log('error:', e);\n      });\n    }\n  },\n  mounted(){\n    let user = localStorage.getItem(\"email_id\");\n    if (user) {\n      this.$router.push({name:'home'})\n    }\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}