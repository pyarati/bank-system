AcA,KAdA,CAcA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,CAAA;AACA,OAhBA;AAiBA;AAtBA,GAfA;AAuCA,EAAA,OAvCA,qBAuCA;AACA,QAAA,IAAA,GAAA,YAAA,CAAA,OAAA,CAAA,IAAA,CAAA;;AACA,QAAA,IAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA;AACA;AA5CA,CAAA","sourcesContent":["<template>\n  <div class=\"login\">\n    <img src=\"../assets/opcito.png\">\n    <h1>Log In</h1>\n    <form>\n    <div class=\"form-group\">\n      <span class=\"text-danger\">*</span> \n      <input type=\"email\" v-model.trim=\"email_id\" placeholder=\"Enter email address\" class=\"form-control\" />\n    </div>\n    <div class=\"form-group\">\n      <span class=\"text-danger\">*</span>\n      <input type=\"password\" v-model=\"password\" placeholder=\"Enter password\" class=\"form-control\"  />\n    </div>\n    <div class=\"form-group\">\n      <button type=\"button\" v-on:click=\"login()\">Login</button>\n    </div>\n    </form>\n    <p>\n      <router-link to='/signup'>Signup</router-link>\n    </p>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport useValidate from '@vuelidate/core'\nimport {required, email} from '@vuelidate/validators'\n\nexport default {\n  name: \"login\",\n  data() {\n    return {\n      v$: useValidate(),\n      email_id: \"\",\n      password: \"\",\n    };\n  },\n  validations(){\n    return {\n      email_id: {required, email},\n      password: {required},\n    };\n  },\n  methods:{\n    login(){\n      const options = {\n        headers: { \"Access-Control-Allow-Origin\": \"*\" },\n      };\n      axios.post('http://0.0.0.0:5000/login', {\n        email_id: this.email_id,\n        password: this.password,\n      },\n      options\n      )\n      .then((result) => {\n        alert(\"form successfully submited\");\n        console.log(\"result:\", result);\n        let data = result.data.data;\n        localStorage.setItem(\"id\", data.id)\n        localStorage.setItem(\"access_token\",'Bearer '+data.access_token);\n        this.$router.push({name:\"home\"});\n      })\n      .catch((e) => {\n        console.log('error:', e);\n      });\n    }\n  },\n  mounted(){\n    let user = localStorage.getItem(\"id\");\n    if (user) {\n      this.$router.push({name:'home'})\n    }\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}